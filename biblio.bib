@phdthesis{Poberezhnik,
    author = "Побережник, С. М.",
    title = "Автоматизація процесів відтворення функціональних залежностей в системах інформаційної підтримки прийняття рішень",
    type = "автореф. дис... канд. техн. наук : 05.13.06",
    school = "Одес. нац. політехн. ун-т",
    address = "О.",
    year = 2003,
    numpages = 19,
    abstract = "Розроблено редукційну модель відтворення лінійно-регресійних залежностей, метод її швидкого параметричного
    синтезу в процесі структурних змін, що дозволяє суттєво знизити рівень трудомісткості та підвищити рівень автоматизації
    синтезу оптимальної структури залежності. Запропоновано засоби автоматизованого підвищення функціональної адаптивності
    лінійних моделей застосування модифікованих степеневих та тригонометричних функціональних рядів. Розроблено загальну
    архітектуру розширюваної системи підтримки прийняття рішень, яка дозволяє динамічно інтегрувати до її складу програмні
    блоки різних типів інтелектуальної обробки даних. Створено програмну реалізацію комплексної системи підтримки прийняття
    рішень і блоку автоматизованого відтворення функціональних залежностей, динамічно інтегрованого до складу розробленої
    системи, з використанням яких розв'язано кілька практичних задач моделювання. Вірогідність автоматично відтворених
    залежностей в розв'язаних задачах - від 73 % до 99,85 %, а часові витрати на конструювання моделей склали від 40 с до 1,5 хв.",
    language = "ukrainian"
}

@phdthesis{Derbenzev,
    author = "Дербенцев, В. Д.",
